        "@vitejs/plugin-react": "^4.3.1",
        "@vitest/coverage-v8": "^2.0.3",
        "@vitest/ui": "^2.0.3",
        "autoprefixer": "^10.4.19",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.5",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.34.4",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-tailwindcss": "^3.17.4",
        "husky": "^9.0.11",
        "hygen": "^6.2.11",
        "jsdom": "^24.1.0",
        "lint-staged": "^15.2.7",
        "postcss": "^8.4.39",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "prisma": "^5.16.2",
        "tailwindcss": "^3.4.4",
        "tsx": "^4.16.2",
        "typescript": "^5.5.3",
        "vite": "^5.3.3",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^2.0.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md,css}": [
            "prettier --write"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/your-org/orchestration-engine.git"
    },
    "keywords": [
        "orchestration",
        "code-generation",
        "ai",
        "langchain",
        "nextjs",
        "trpc",
        "supabase",
        "typescript"
    ],
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
