        "react-dom": "^18.3.1",
        "react-dropzone": "^14.2.3",
        "react-hook-form": "^7.52.1",
        "react-hot-toast": "^2.4.1",
        "recharts": "^2.12.7",
        "sonner": "^1.5.0",
