{
    "name": "orchestration-engine",
    "version": "0.1.0",
    "description": "AI-powered code generation orchestration engine with Next.js, tRPC, and Supabase",
    "private": "true",
    "author": "LESiAB",
    "license": "MIT",
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.0.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "format": "prettier --write",
        "format:check": "prettier --check",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "db:generate": "supabase gen types typescript --local > lib/db/types.ts",
        "db:push": "supabase db push",
        "db:migrate": "supabase migration up",
        "db:reset": "supabase db reset",
        "db:seed": "tsx scripts/seed.ts",
        "prepare": "husky install",
        "analyze": "ANALYZE=true next build",
        "engine:test": "vitest run src/engine/**/*.test.ts",
        "codegen:crud": "hygen crud new",
        "shadcn": "npx shadcn@latest"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.24.3",
